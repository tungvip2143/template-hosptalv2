{
  "name": "remote-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=20.10 <23"
  },
  "packageManager": "npm@10",
  "scripts": {
    "check-pnkx": "node scripts/check-update-pnkx.cjs",
    "dev": "npm run check-pnkx && vite --mode development --host",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "build:css": "npx @tailwindcss/cli -i ./src/styles/index.css -o ./public/index.css --minify",
    "build": "npm run typecheck && npm run build:css && npm run clean && vite build --mode production",
    "build:staging": "npm run typecheck && npm run build:css && npm run clean && vite build --mode staging",
    "lint": "eslint .",
    "preview": "vite preview --host --strictPort"
  },
  "dependencies": {
    "@module-federation/vite": "^1.2.7",
    "@pnkx-lib/core": "^1.1.217",
    "@pnkx-lib/icon": "0.0.87",
    "@pnkx-lib/ui": "^1.9.542",
    "@tailwindcss/cli": "^4.1.6",
    "@tailwindcss/vite": "^4.0.17",
    "@tanstack/react-query": "^5.75.4",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "react-hook-form": "^7.57.0",
    "react-i18next": "^15.7.3",
    "tailwindcss": "^4.0.17",
    "vite-plugin-compression": "^0.5.1",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@tanstack/react-query-devtools": "^5.76.1",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.15.3",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "chokidar": "^4.0.3",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router": "7.6.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.2.6"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.6.0"
  }
}
